<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.gooalgene.common.dao.IndexExplainDao">

    <select id="findList" resultType="com.gooalgene.entity.IndexExplain">
        SELECT * FROM index_explain;
    </select>

    <select id="findByType" resultType="com.gooalgene.entity.IndexExplain">
        SELECT * FROM index_explain WHERE type = #{type}
    </select>

    <update id="update" parameterType="com.gooalgene.entity.IndexExplain">
        UPDATE index_explain SET detail=#{detail} WHERE id=#{id};
    </update>

</mapper>