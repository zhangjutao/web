<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
        <mapper namespace="com.gooalgene.common.dao.LoginInfoDao">
    <resultMap id="LoginInfo" type="com.gooalgene.common.authority.LoginInfo">
        <result column="userid" property="id" jdbcType="INTRGER"/>
        <result column="logintime" property="loginTime" jdbcType="TIMESTAMP"/>
        <result column="logouttime" property="logoutTime" jdbcType="TIMESTAMP"/>
    </resultMap>

    <insert id="insertLoginInfo">
        INSERT  INTO  login_info (userid,logintime,logouttime)VALUE (#{id},#{loginTime},#{logoutTime})
    </insert>

    <select id="getLoginInfoById" resultMap="LoginInfo">
        SELECT * FROM login_info WHERE userid=#{id}
    </select>

    <select id="getAllLoginInfo" resultMap="LoginInfo">
        SELECT  * FROM  login_info
    </select>

</mapper>