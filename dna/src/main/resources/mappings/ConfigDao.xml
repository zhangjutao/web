<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.gooalgene.common.dao.ConfigDao">
    <select id="getAllConfig" parameterType="list" resultType="com.gooalgene.entity.Configuration">
        SELECT
            prop_key as "key",
            prop_value as "value"
        FROM db_config
    </select>

</mapper>